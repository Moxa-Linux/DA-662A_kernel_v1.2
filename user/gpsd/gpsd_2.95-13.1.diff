@@ -0,0 +1,22 @@
+#! /bin/sh
+# postrm script for gpsd
+
+set -e
+
+case "$1" in
+    purge)
+	[ ! -f /etc/default/gpsd ] || rm /etc/default/gpsd
+    ;;
+
+    remove|upgrade|failed-upgrade|abort-install|disappear|abort-upgrade)
+    ;;
+
+    *)
+        echo "postrm called with unknown argument \`$1'" >&2
+        exit 1
+
+esac
+
+#DEBHELPER#
+
+exit 0
